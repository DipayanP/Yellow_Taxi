{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dpdataenggadf02"
		},
		"DippsAzKeyVltLinkedService01_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dippskeyvault01.vault.azure.net/"
		},
		"DippsAzKeyVltLinkedService02_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dippskeyvault02.vault.azure.net/"
